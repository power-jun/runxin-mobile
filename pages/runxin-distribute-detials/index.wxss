.container {
  padding: 25rpx;
}

.credit-agency {
  width: 100%;
  padding: 30rpx;
  padding-bottom: 0;
  background: #fff;
  box-shadow: 0rpx 2rpx 10rpx #ccc;
  box-sizing: border-box;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}

.agency-company {
  font-size: 22rpx;
  color: #7b8389;
  padding-bottom: 25rpx;
  border-bottom: 1px solid #e5e5e5;
  margin-bottom: 30rpx;
}

.agency-company.no-border {
  border: none;
  padding: 0;
}

.holding-list {
  width: 100%;
}

.details-img-box {
  width: 100%;
  margin-top: 25rpx;
}

.details-img-list image {
  display: inline-block;
  width: 115rpx;
  height: 115rpx;
  border: 2rpx solid #ddd;
  margin-left: 20rpx;
}

.details-img-box .tips {
  font-size: 20rpx;
  color: #7b8389;
}

.protocol {
  text-align: center;
  margin: 50rpx 0 80rpx 0;
  font-size: 26rpx;
}

.protocol text {
  color: #2990e6;
}

.protocol input {
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
}

.submit-btn-box {
  display: flex;
}

.submit-btn-box button {
  width: 170rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  color: #fff;
  font-size: 28rpx;
}

.submit-btn {
  background: #cf4141;
  margin-right: 125rpx;
}

.cancel-btn {
  background: #a1a1a1;
}

.generic {
  width:100%;
  margin: 15px;
  text-align: right;
}

.generic .text {
  margin-left: 10px;
  font-size: 14px;
  color: #919598;
  line-height: 1.2;
}
