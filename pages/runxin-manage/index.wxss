.type-navbar {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 500;
  display: flex;
  height: 50px;
  border-top: 1px solid #ededed;
  background-color: #fff;
}

/* ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
} */

.type-box {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
}

.type-navbar-item {
  padding: 15px 0;
  font-size: 14px;
  color: #23303a;
  line-height: 1.2;
  border-bottom: 4px solid #fff;
}

.type-item-on {
  color: #cd3a3a;
  border-bottom: 8rpx solid #cd3a3a;
}

.holding-list {
  display: block;
}

.article {
  padding: 51px 0 1px;
}

.transaction-inquiry {
  margin: 15px;
}

.list-type {
  display: flex;
}

.type-item {
  flex: 1;
  padding: 5px 0;
  border: 1px solid #ebbfbf;
  color: #cd3a3a;
  line-height: 1.2;
  text-align: center;
  font-size: 14px;
  border-radius: 5px;
  margin-right: 15px;
}

.type-item:last-child {
  margin: 0;
}

.type-item-cur {
  border-color: #cd3a3a;
  color: #fff;
  background: #cd3a3a;
}

.advanced-search {
  margin: 15px;
  padding: 10px;
  font-size: 14px;
  border-bottom: 1px solid #eee;
  line-height: 1.2;
  color: #cd3a3a;
  text-align: center;
  background: #f4e5e5;
  border-radius: 5px;
}

.credit-agency {
  margin: 15px;
  padding: 5px 15px;
  background: #fff;
  box-shadow: 0rpx 2rpx 10rpx #ccc;
  box-sizing: border-box;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}

.agency-company {
  padding: 15px 0;
  font-size: 14px;
  color: #7b8389;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  justify-content: space-between;
}

.agency-company:last-child {
  border: 0;
}

.agency-company text:first-child {
  color: #7b8389;
}

.container {
  overflow: hidden;
}
