<scroll-view class='body' scroll-y='true'>
  <view class='credit-agency'>
    <view class='agency-company'>
      <text>上一转让人：</text>
      <text>深圳宇宙有限公司</text>
    </view>
    <view class='agency-company'>
      <text>收信经办：</text>
      <text>黄先</text>
    </view>
    <view class='agency-company'>
      <text>办理时间：</text>
      <text>2018-01-23 手机端</text>
    </view>
    <view class='agency-company no-border'>
      <text>是否为拆转或折融子信</text>
      <radio-group class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="1" checked="true" />是
          <radio value="1" checked="" />否
        </label>
      </radio-group>
    </view>
  </view>
  <view class='article'>
    <master-mask/>
  </view>
  <view class='footer-fixed'>
    <view class='footer'>
      <button class='ok' bindtap='gotoFinancing'>融资</button>
      <button class='cancel' bindtap='gotoTransfer'>转让</button>
    </view>
  </view>
</scroll-view>
